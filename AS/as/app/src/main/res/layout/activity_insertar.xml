<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".insertar">

    <Button
        android:id="@+id/btnRegistrar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="Registrar"
        app:layout_constraintTop_toBottomOf="@+id/cajaCorreo"
        tools:layout_editor_absoluteX="146dp" />

    <EditText
        android:id="@+id/cajaDocumento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="88dp"
        android:ems="10"
        android:inputType="text"
        android:text="Documento"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="100dp" />

    <EditText
        android:id="@+id/cajaNombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:ems="10"
        android:inputType="text"
        android:text="Nombre"
        app:layout_constraintTop_toBottomOf="@+id/cajaDocumento"
        tools:layout_editor_absoluteX="100dp" />

    <EditText
        android:id="@+id/cajaContrasenha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:ems="10"
        android:inputType="text"
        android:text="Contraseña"
        app:layout_constraintTop_toBottomOf="@+id/cajaNombre"
        tools:layout_editor_absoluteX="100dp" />

    <EditText
        android:id="@+id/cajaTelefono"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:ems="10"
        android:inputType="text"
        android:text="Teléfono"
        app:layout_constraintTop_toBottomOf="@+id/cajaContrasenha"
        tools:layout_editor_absoluteX="100dp" />

    <EditText
        android:id="@+id/cajaCorreo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:ems="10"
        android:inputType="text"
        android:text="Correo"
        app:layout_constraintTop_toBottomOf="@+id/cajaTelefono"
        tools:layout_editor_absoluteX="100dp" />
</androidx.constraintlayout.widget.ConstraintLayout>